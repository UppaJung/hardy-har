{
  "name": "@stuartschechter/hardy-har",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "tasks": {
    "build-npm": "deno run -A ./scripts/build-npm-package.ts"
  },
  "imports": {
  },
  "lint": {
    "include": ["lib/"],
    "exclude": ["test/"],
    "rules": {
      "tags": ["recommended"],
      "include": ["ban-untagged-todo", "no-slow-types"],
      "exclude": []
