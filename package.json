{
  "name": "@uppajung/hardy-har",
  "type": "module",
  "version": "0.1.1",
  "description": "A Hardy HTTP Archive (HAR) Generator ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "node --import tsx --test test/*.test.ts",
    "test:vscode-only-har": "node --import tsx --test test/debugger-events-to-har/test/chrome-har-tests.test.ts",
    "test:vscode": "node --import tsx --test test/**/*.test.ts test/**/**/*.test.ts",
    "build": "rm -rf dist && node esbuild.js && tsc --build",
    "clean": "tsc --build --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UppaJung/hardy-har.git"
  },
  "keywords": [
    "http",
    "archive",
    "har",
    "typescript"
  ],
  "author": "Stuart Schechter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UppaJung/hardy-har/issues"
  },
  "homepage": "https://github.com/UppaJung/hardy-har",
  "dependencies": {
    "@types/har-format": "^1.2.15",
    "@types/node": "^22.7.3",
    "devtools-protocol": "^0.0.1359167",
    "esbuild": "^0.24.0",
    "esbuild-node-externals": "^1.15.0",
    "tough-cookie": "^5.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.272",
    "chrome-har": "^0.13.5",
    "expect": "^29.7.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
